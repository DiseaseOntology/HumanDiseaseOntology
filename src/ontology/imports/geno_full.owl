    <!-- http://purl.obolibrary.org/obo/GENO_0000149 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GENO_0000149">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GENO_0000148"/>
